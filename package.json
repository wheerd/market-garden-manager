{
  "name": "market-garden-manager",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://wheerd.github.io/market-garden-manager",
  "type": "module",
  "engines": {
    "node": ">=20.12.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "pnpm lint --fix",
    "*.{html,css,scss,md,json,yml,yaml}": "prettier --write"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "husky",
    "test": "vitest",
    "test:ci": "vitest run",
    "coverage": "vitest run --coverage",
    "coverage:ci": "vitest run --coverage.enabled --coverage.reporter=cobertura"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "chart.js": "^4.4.2",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-plugin-annotation": "^3.0.1",
    "date-fns": "^3.6.0",
    "idb-keyval": "^6.2.1",
    "leaflet": "^1.9.4",
    "leaflet-geosearch": "^3.11.1",
    "leaflet.locatecontrol": "^0.79.0",
    "openmeteo": "^1.1.3",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.2",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-loading-skeleton": "^3.4.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@testing-library/react": "^14.2.2",
